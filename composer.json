{
    "name": "gyselroth/tubee",
    "description": "Tubee",
    "keywords": ["import", "export", "data-management"],
    "homepage": "https://github.com/gyselroth/tubee",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Raffael Sahli",
            "email": "sahli@gyselroth.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tubee\\": "src/lib",
            "Tubee\\Testsuite\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/raffis/php-mongomock"
        }
    ],
    "require": {
        "php": ">=7.2",
        "psr/log": "1.*",
        "ext-mongodb": "*",
        "ext-curl": "*",
        "ext-apcu": "*",
        "gyselroth/micro-auth": "*",
        "gyselroth/micro-container": "^2.0.0",
        "gyselroth/micro-http": "^0.0.6",
        "psr/container": "*",
        "mongodb/mongodb": "^1.4.0",
        "ulrichsg/getopt-php": "^3.0",
        "zendframework/zend-mail": "2.9.*",
        "gyselroth/mongodb-php-task-scheduler": "^3.0.0",
        "hassankhan/config": "*",
        "symfony/yaml": "~3.1",
        "monolog/monolog": "^1.23",
        "bramus/monolog-colored-line-formatter": "~2.0",
        "zendframework/zend-servicemanager": "^3.3",
        "icewind/smb": "^2.0",
        "symfony/expression-language": "^4.0",
        "psr/cache": "^1.0",
        "dreamscapes/ldap-core": "^3.1",
        "guzzlehttp/psr7": "^1.4",
        "mindplay/middleman": "^3.0",
        "lcobucci/content-negotiation-middleware": "^1.0",
        "zendframework/zend-diactoros": "^1.7",
        "psr/http-server-middleware": "*",
        "middlewares/utils": "^1.0",
        "middlewares/trailing-slash": "^1.0",
        "middlewares/access-log": "^1.0",
        "middlewares/payload": "^2.0",
        "middlewares/negotiation": "^1.0",
        "nikic/fast-route": "^1.3",
        "middlewares/fast-route": "^1.0",
        "php-jsonpatch/php-jsonpatch": "^3.0",
        "stolt/json-merge-patch": "^1.0",
        "psr/http-message": "^1.0",
        "gyselroth/stream-iterator": "dev-master",
        "guzzlehttp/guzzle": "~6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "ext-ldap": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-imagick": "*",
        "ext-xml": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpunit/phpunit": "^6.0.0",
        "helmich/mongomock": "dev-master#bf141f5",
        "phpstan/phpstan": "*"
    }
}
