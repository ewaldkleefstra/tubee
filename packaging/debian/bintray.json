{
    "package": {
        "name": "tubee",
        "repo": "tubee",
        "subject": "gyselroth",
        "desc": "tubee cloud server",
        "website_url": "https://github.com/gyselroth/tubee",
        "issue_tracker_url": "https://github.com/gyselroth/tubee/issues",
        "vcs_url": "https://github.com/gyselroth/tubee.git",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "CHANGELOG.md",
        "licenses": ["GPL-3.0"],
        "labels": ["tubee", "i/o", "proxy", "data-management", "sync", "php"],
        "public_download_numbers": true,
        "public_stats": true
    },
    "version": {
        "name": "{version}",
        "released": "{date}",
        "gpgSign": true
    },
    "files": [
        {"includePattern": "dist/(.*\.deb)", "uploadPattern": "$1",
        "matrixParams": {
            "deb_distribution": "{deb_distribution}",
            "deb_component": "main",
            "deb_architecture": "all",
            "override": 1}
        }
    ],
    "publish": true
}
